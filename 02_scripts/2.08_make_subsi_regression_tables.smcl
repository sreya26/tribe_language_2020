{txt}  5{com}.                                         else if "`area'" == "protected" local ifcond "if scheduledarea6113~=0 | tribalarea6113~=0"
{txt}  6{com}.                                                 else if "`area'" == "unprotected" local ifcond "if scheduledarea6113 == 0 & tribalarea6113==0"
{txt}  7{com}.                                 
.                 foreach dtype of local dist_types {c -(}
{txt}  8{com}.                         foreach type of local match_to {c -(}
{txt}  9{com}.                                 egen lang_dist_normalized = std(wt_lang_dist_`dtype'_`type') `ifcond'
{txt} 10{com}.                                 label var lang_dist_normalized "Distance"
{txt} 11{com}.                                 gen lang_dist_norm_sq = lang_dist_normalized^2
{txt} 12{com}.                                 label var lang_dist_norm_sq "Distance\$^2\$"
